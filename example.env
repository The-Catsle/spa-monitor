# Arctic Spa API Token (from your spa account)
SPA_API_TOKEN=your_spa_api_token_here

# Pushover Configuration
# Get these from https://pushover.net/
PUSHOVER_API_TOKEN=your_pushover_api_token_here
PUSHOVER_USER_KEY=your_pushover_user_key_here

# Optional Pushover Settings
# Priority levels: -2 (lowest) to 2 (highest), default is 0
PUSHOVER_PRIORITY=0
# Custom title for notifications
PUSHOVER_TITLE="Spa Monitor"